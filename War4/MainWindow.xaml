<Window x:Class="War4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:War4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Calendar x:Name="calendar" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" SelectedDatesChanged="Calendar_SelectedDatesChanged"/>
        <DatePicker x:Name="datepicker" HorizontalAlignment="Left" Margin="232,14,0,0" VerticalAlignment="Top" SelectedDateChanged="datepicker_SelectedDateChanged"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="10,265,0,0" TextWrapping="Wrap" Text="Дата 1" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="10,312,0,0" TextWrapping="Wrap" Text="Дата 2" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock2" HorizontalAlignment="Left" Margin="383,268,0,0" TextWrapping="Wrap" Text="Онлайн дата" VerticalAlignment="Top"/>

    </Grid>
</Window>
